let delim = [' ' '\t' '\n']
let ws = delim+
let letter = ['A'-'Z' 'a'-'z']
let digit = ['0'-'9']
let ID = letter(letter|digit)*

rule tokens = 
    ws       { return WS }
  | ID        { return ID }
  | '+'       { return PLUS }
  | '*'       { return TIMES }
  | '('       { return LPAREN }
  | ')'       { return RPAREN }
